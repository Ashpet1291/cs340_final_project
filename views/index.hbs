<!-- index.hbs -->
<h1>The server is running!</h1>
<p>If you see this text, the server is <strong>definitely</strong> working!</p>

{{!-- Create a table --}}
<table>

    {{!-- Header section --}}
    <thead>

        <tr>
            {{#each data.[0]}}
            <th>
                {{@key}}
            </th>
            {{/each}}
        </tr>
    </thead>
        {{!-- Body section --}}
        <tbody>
            {{#each data}}
            <tr>
               <td>{{this.family_id}}</td>
               <td>{{this.active}}</td>
               <td>{{this.first_name}}</td>
               <td>{{this.nick_name}}</td>
               <td>{{this.last_name}}</td>
               <td>{{this.birthday}}</td>
               <td>{{this.primary_number}}</td>
            </tr>
            {{/each}}
        </tbody>
</table>
