<h1>The server is running!</h1>
<p>If you see this text, the server is <strong>definitely</strong> working!</p>
{{!-- Create a table --}}
<table>

    {{!-- Header section --}}
        <thead>

            <tr>
                {{#each data.[0]}}
                <th>
                    {{@key}}
                </th>
                {{/each}}
            </tr>                                                                           </thead>

      {{!-- Body section --}}
      <tbody>

         {{#each data}}
         <tr>
            <td>{{this.place_id}}</td>
            <td>{{this.active}}</td>
            <td>{{this.name}}</td>
            <td>{{this.address}}</td>
            <td>{{this.website}}</td>
            <td>{{this.indoor}}</td>
            <td>{{this.note}}</td>
         </tr>
         {{/each}}
      </tbody>
</table>
